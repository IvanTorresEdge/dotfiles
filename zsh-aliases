alias ls='ls -hG'
alias ll='ls -lhG'
alias la='ls -lahG'

alias vim='nvim'
alias python='/usr/local/bin/python3'
alias pip='/usr/local/bin/pip3'
alias python3='/usr/local/bin/python3'
alias pip3='/usr/local/bin/pip3'
alias python2='/usr/local/bin/python2'
alias pip2='/usr/local/bin/pip2'

# Sprinklr
alias pw='prettier --write $1'
alias j='jest --config ./jest.config.json $1'
alias jw='jest --config ./jest.config.json --watch $1'
